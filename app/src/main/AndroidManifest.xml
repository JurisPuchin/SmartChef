<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="t4.csc413.smartchef" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".MapsActivity"
            android:label="Find a Store" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>

        <activity android:name=".Results"></activity>
        <activity android:name=".Results_one"></activity>
        <activity android:name=".Results_two"></activity>
        <activity android:name=".Results_three"></activity>
        <activity android:name=".Results_four"></activity>
        <activity android:name=".Results_five"></activity>
        <activity android:name=".Results_six"></activity>
        <activity android:name=".Results_seven"></activity>
        <activity android:name=".Results_eight"></activity>
        <activity android:name=".Results_nine"></activity>
        <activity android:name=".Results_ten"></activity>
        <activity android:name=".The_Results"></activity>


        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

    </application>



</manifest>